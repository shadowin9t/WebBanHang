<?xml version="1.0"?>
<configuration>
  <system.web>
    <compilation debug="true" targetFramework="4.5.2" />
    <httpRuntime targetFramework="4.5.2" />
    <authentication mode="Forms">
    </authentication>
  </system.web>
<location path="Admin">
  <system.web>
    <authorization>
      <allow roles="admin"/>
      <deny users="*"/>
    </authorization>
  </system.web>
</location>
<location path="Admin/User">
  <system.web>
    <authorization>
      <allow roles="USER_ADMIN, SUPER_ADMIN"/>
      <deny users="*"/>
    </authorization>
  </system.web>
</location>


</configuration>
